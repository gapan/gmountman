#: src/gmountman.glade:25 gmountman.desktop.in.h:2
msgid "Mount Manager"
msgstr "Διαχειριστής Προσάρτησης Συσκευών"

#: src/gmountman.glade:116 src/gmountman:488 src/gmountman:542
msgid "Mount"
msgstr "Προσάρτηση"

#: src/gmountman.glade:132 src/gmountman:481 src/gmountman:544
msgid "Unmount"
msgstr "Αποπροσάρτηση"

#: src/gmountman.glade:148 src/gmountman:492 src/gmountman:546
msgid "Eject"
msgstr "Εξαγωγή"

#: src/gmountman.glade:164 src/gmountman:548
msgid "Open"
msgstr "Άνοιγμα"

#: src/gmountman.glade:189 src/gmountman.glade:977 src/gmountman:550
msgid "Preferences"
msgstr "Προτιμήσεις"

#: src/gmountman.glade:274 gmountman.desktop.in.h:1
msgid "A mount manager for local devices that uses gvfs"
msgstr "Ένας διαχειριστής προσάρτησης τοπικών συσκεύων με τη χρήση του gvfs"

#: src/gmountman.glade:952
msgid "translator-credits"
msgstr "Γιώργος Βλαχάβας <vlahavas~at~gmail~dot~com>"

#: src/gmountman.glade:1002
msgid "<b>Open mounted devices with:</b>"
msgstr "<b>Άνοιγμα των προσαρτημένων συσκευών με:</b>"

#: src/gmountman.glade:1011
msgid "Default file manager (specified by XDG)"
msgstr "Εξ ορισμού διαχειριστή αρχείων (όπως ορίζεται από το XDG)"

#: src/gmountman.glade:1029
msgid "Custom file manager:"
msgstr "Άλλο διαχειριστή αρχείων:"

#: src/gmountman.glade:1069
msgid "<b>Notifications:</b>"
msgstr "<b>Ειδοποιήσεις</b>"

#: src/gmountman.glade:1078
msgid "Display a notification when a device has been unmounted"
msgstr "Εμφάνιση ειδοποίησης όταν μια συσκευή έχει αποπροσαρτηθεί"

#: src/gmountman:169
msgid "Device is now safe to remove"
msgstr "Η συσκευή είναι έτοιμη για αφαίρεση"

#: src/gmountman:169
#, python-format
msgid "The device %s can now be safely removed"
msgstr "Η συσκευή %s μπορεί τώρα να αφαιρεθεί με ασφάλεια"

#: src/gmountman:501
msgid "Hide"
msgstr "Απόκρυψη"

#: src/gmountman:503
msgid "Show"
msgstr "Εμφάνιση"

#: src/gmountman:557
msgid "Name"
msgstr "Όνομα"

#: src/gmountman:559
msgid "Device"
msgstr "Συσκευή"

#: src/gmountman:561
msgid "Mount Point"
msgstr "Σημείο Προσάρτησης"
